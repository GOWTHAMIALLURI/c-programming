// Online C compiler to run C program online

#include <stdio.h>
int main() 
{
int n,k,i,t;
 printf("enter n value:");
 scanf("%d",&n);
 t=n;
 n=0;
 for(k=1;k>=0;k++)
 {
  for(i=1;i<=k;i++)
  {
  n=n+1;
  if(n==t)
  break;
 }
if(n==t)
break;
}
printf("\n%d",k-1);
return 0;
}

output:
enter n value:5
2
