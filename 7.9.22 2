// Online C compiler to run C program online
#include <stdio.h>
#include<stdlib.h>

int main() {
    int n,*p,sum=0,i;
    printf("enter size:");
    scanf("%d",&n);
    p=(int *)malloc(n*sizeof(int));
    for(i=0;i<n;i++)
    scanf("%d",(p+i));
    for(i=0;i<n;i++)
    {
        sum=sum+*(p+i);
    }
    printf("sum=%d",sum);
    return 0;
}
