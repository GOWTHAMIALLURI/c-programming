#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char a[100];
    int x[10]={0},i,val;
    gets(a);
    for(i=0;a[i]!='\0';i++)
    {
        if((a[i]>='0')&&(a[i]<='9'))
        {
        val=a[i]-'0';
        x[val]=x[val]+1;
        }
    }
        
    for(i=0;i<10;i++)
    printf("%d ",x[i]);
    return 0;
}
