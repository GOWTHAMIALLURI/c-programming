R = int(input("number of rows:"))
C = int(input("number of columns:"))
matrix = []
print("Enter the values:")
for i in range(R):  
    a =[]
    for j in range(C):      
         a.append(int(input()))
    matrix.append(a)
for i in range(R):
    for j in range(C):
        print(matrix[i][j], end = " ")
    print()
