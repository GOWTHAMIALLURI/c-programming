// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,i,j;
    printf("enter n value:");
    scanf("%d",&n);
    for(i=n;i>=1;i--)
    {
        printf("\n");
        for(j=n;j>=i;j--)
        {
        printf("%d",j);
        }
    }
    return 0;
}

//output:
/tmp/9rMBpAT8YB.o
enter n value:6
6
65
654
6543
65432
654321
