// Online C compiler to run C program online
#include <stdio.h>
char * evenorodd(int);
int main() {
    int n;
    char *s;
    printf("enter number:");
    scanf("%d",&n);
    s=evenorodd(n);
    printf("%s",s);
}
 char * evenorodd(int n)
{
    if(n%2==0)
    return("even");
    else
    return("odd");
}

    
